{
"GATHERED_AT": "{{ ansible_date_time.date }} {{ ansible_date_time.time }}",
"HOSTNAME": "{{ ansible_fqdn }}",
"IPADDRESS": "{{ ansible_default_ipv4.address }}",
"OS_VENDOR": "{{ ansible_distribution }}",
"OS_VERSION": "{{ ansible_distribution_version }}",
"OS_KERNEL_VERSION": "{{ ansible_kernel }}",
"OS_RELEASE": "{{ ansible_distribution_release }}",
"SYSTEM_VENDOR": "{{ system_vendor | default('Unknown') }}",
"NETWORK_INTERFACES": "{{ ansible_facts.interfaces | sort }}",
"SYSTEMD_SERVICES": "{{ systemd_services_info | default([]) }}",
"FS_MOUNTS": "{{ mounts_info | sort }}"
}
